{
  "Files": [
    "consul.config"
  ],
  "Dirs": [
    "etc"
  ],
  "Args": [
    "agent",
    "-config-file=/consul.config",
    "-config-format=json"
  ],
  "RunConfig": {
    "Ports": [
      "53",
      "8300",
      "8301",
      "8302",
      "8400",
      "8500"
    ],
    "ShowDebug": true,
    "Klibs": ["syslog"]
  },
  "ManifestPassthrough": {
    "syslog": {
      "server": "consul-logger.service.consul"
    }
  },
  "CloudConfig": {
    "Zone": "eu-west-1",
    "Platform": "aws"
  }
}
